<?xml version="1.0" encoding="utf-8"?>
<!-- Root is "Hotel" instead of "Hotels" -->
<Hotel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="https://github.com/DaBestCode/XML-XSD.xsd">

  <Hotel ID="H1" Category="Luxury">
    <!-- Provide two <Name> tags -->
    <Name>Westin</Name>
    <Name>AnotherName</Name>
    <!-- Missing closing </Address> -->
    <Address>
      <Number>11</Number>
      <Street>E 7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
      <NearestAirport>Sky Harbor</NearestAirport>
    <!-- No Phone element at all -->
    <Rating>4.2</Rating>
  </Hotel>

  <!-- Another <Hotel> missing the required "ID" attribute -->
  <Hotel Category="Budget">
    <Name>Courtyard</Name>
    <Phone>480-555-1234</Phone>
  </Hotel>

</Hotel>
